<script>
  const valueToWordMapping = ["", "Poor", "Below Average", "Average", "Good", "Excellent"];
  const wordToColorMapping = {
    "Poor": "red",
    "Below Average": "orange",
    "Average": "green",
    "Good": "blue",
    "Excellent": "yellow"
  };

  // Function to update the slider value display for a specific slider
  const updateSliderValueDisplay = (slider, valueDisplay) => {
    const sliderValue = parseInt(slider.value);
    const word = valueToWordMapping[sliderValue];
    valueDisplay.textContent = word;

    // Update the color based on the word
    valueDisplay.style.color = wordToColorMapping[word] || 'rgb(251, 148, 111)'; // default color
  };
  
  // When the document is loaded, set up event listeners for all sliders
  document.addEventListener("DOMContentLoaded", () => {
    const sliders = document.querySelectorAll(".slider");
    const sliderValueDisplays = document.querySelectorAll(".slider-value");
  
    // Event listener for each slider to update its value display when interacted with
    sliders.forEach((slider, index) => {
      updateSliderValueDisplay(slider, sliderValueDisplays[index]);
  
      slider.addEventListener("input", () => {
        updateSliderValueDisplay(slider, sliderValueDisplays[index]);
      });
    });
  });
</script>




