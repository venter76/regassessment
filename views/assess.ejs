 /* Custom styling for the slider container */
 .slider-container {
  
    display: flex;
    align-items: center;
    padding-right: 10%;
  }

  /* Custom styling for the slider */
  .slider {
  
    width: 90% !important;
    
  }

  /* Custom styling for the number display */
  .slider-value {
    color: rgb(251, 148, 111);
    min-width: 40px;
    text-align: center;
    font-weight: bold;
    font-size: 18px;
  }

   
  /* Custom styling for the slider track */
  .slider-track {

    width: 100%;
    height: 1px;
    border-radius: 3px;
    background-color: #313437; /* Original blue color */
    transition: background-color 0.2s ease-in-out;
  }

  
